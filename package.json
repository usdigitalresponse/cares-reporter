{
  "name": "cares-reporter",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:oh": "POSTGRES_URL=postgres://localhost/ohio UPLOAD_DIRECTORY=\"./uploads/oh\" TREASURY_DIRECTORY=\"./treasury-reports/oh\" REPORTING_TEMPLATE=\"OBM Reporting Template.xlsx\" yarn serve",
    "serve:ri": "POSTGRES_URL=postgres://localhost/ri UPLOAD_DIRECTORY=\"./uploads/ri\" TREASURY_DIRECTORY=\"./treasury-reports/ri\" REPORTING_TEMPLATE=\"RICRF Reporting Workbook for Projects Not Previously Reported.xlsx\" yarn serve",
    "serve:rp1": "POSTGRES_URL=postgres://localhost/rptest AUDIT=true UPLOAD_DIRECTORY=\"./uploads/rp\" TREASURY_DIRECTORY=\"./treasury-reports/rp\" yarn serve",
    "serve:rp2": "POSTGRES_URL=postgres://localhost/rp2 AUDIT=true UPLOAD_DIRECTORY=\"./uploads/rp\" TREASURY_DIRECTORY=\"./treasury-reports/rp\" yarn serve",
    "reset:rptest": "dropdb rptest && createdb --owner=postgres rptest && psql rptest postgres < tests/server-periods/fixtures/rptest.sql",
    "reset:rp1": "dropdb rptest && createdb --owner=postgres rp1 && psql rptest postgres < tests/server-periods/fixtures/rptest.sql",
    "reset:rp2": "dropdb rp2 && createdb --owner=postgres rp2 && psql rp2 postgres < tests/server-periods/fixtures/rp2.sql",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:server": "./tests/server/mocha_wrapper.sh",
    "test:server-period-1": "./tests/server-period-1/mocha_wrapper.sh",
    "test:server-period-2": "./tests/server-period-2/mocha_wrapper.sh",
    "test:server-nodemailer": "./tests/server-nodemailer/mocha_wrapper.sh",
    "test:server-aws-ses": "./tests/server-nodemailer/mocha_wrapper.sh",
    "lint": "vue-cli-service lint",
    "prod": "NODE_ENV=production node src/server"
  },
  "dependencies": {
    "@vue/cli": "^4.5.13",
    "aws-sdk": "^2.963.0",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.16.0",

    "date-fns": "^2.23.0",
    "dotenv": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",

    "knex": "^2.4.0",
    "lodash": "^4.17.21",
    "lodash-checkit": "^2.3.3",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mustache": "^4.2.0",
    "nodemailer": "^6.6.3",
    "numeral": "^2.0.6",
    "pg": "^8.7.1",
    "standard": "^16.0.3",
    "uuid": "^8.3.2",
    "vue": "^2.6.13",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "mocha": "^8.1.2",
    "prettier": "1.19.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "4.0.0"
  }
}
